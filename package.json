{
  "name": "talohana.com",
  "private": true,
  "version": "1.0.0",
  "author": "Tal Ohana <tal.ohana.x@gmail.com>",
  "engines": {
    "node": ">= 14.15.0"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "commit": "git-cz",
    "codegen": "graphql-codegen --config codegen.yml",
    "postcodegen": "prettier --write ./src/types/generated.ts"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.19",
    "@mdx-js/react": "^1.6.19",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "gatsby": "^2.24.91",
    "gatsby-background-image": "^1.1.2",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-google-analytics": "^2.3.19",
    "gatsby-plugin-manifest": "^2.4.35",
    "gatsby-plugin-mdx": "^1.2.52",
    "gatsby-plugin-offline": "^3.2.29",
    "gatsby-plugin-react-helmet": "^3.3.12",
    "gatsby-plugin-sharp": "^2.6.43",
    "gatsby-plugin-sitemap": "^2.4.17",
    "gatsby-plugin-styled-components": "^3.3.14",
    "gatsby-plugin-typography": "^2.5.12",
    "gatsby-remark-autolink-headers": "^2.3.15",
    "gatsby-remark-copy-linked-files": "^2.3.19",
    "gatsby-remark-embedder": "^3.0.0",
    "gatsby-remark-images": "^3.3.39",
    "gatsby-remark-prismjs": "^3.5.16",
    "gatsby-remark-table-of-contents": "^0.1.1",
    "gatsby-source-filesystem": "^2.3.35",
    "gatsby-transformer-sharp": "^2.5.20",
    "lodash": "^4.17.20",
    "polished": "^3.6.7",
    "prismjs": "^1.22.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^3.11.0",
    "react-typography": "^0.16.19",
    "remark-emoji": "^2.1.0",
    "styled-components": "^5.2.0",
    "styled-media-query": "^2.1.2",
    "typography": "^0.16.19",
    "typography-theme-irving": "^0.16.19",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@graphql-codegen/cli": "1.19.0",
    "@graphql-codegen/typescript": "1.17.11",
    "@types/mdx-js__react": "^1.5.3",
    "@types/react-helmet": "^6.1.0",
    "@types/styled-components": "^5.1.4",
    "@types/typography": "^0.16.3",
    "commitizen": "^4.2.2",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2",
    "prettier": "2.1.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  }
}
