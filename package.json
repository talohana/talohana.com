{
  "name": "talohana.com",
  "private": true,
  "version": "1.0.0",
  "author": "Tal Ohana <tal.ohana.x@gmail.com>",
  "engines": {
    "node": ">= 14.15.0"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx,css,scss,yaml,yml}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "commit": "git-cz",
    "codegen": "graphql-codegen --config codegen.yml",
    "postcodegen": "prettier --write ./src/types/generated.ts"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.19",
    "@mdx-js/react": "^1.6.19",
    "gatsby": "^2.26.1",
    "gatsby-background-image": "^1.1.2",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-google-analytics": "^2.3.19",
    "gatsby-plugin-manifest": "^2.4.35",
    "gatsby-plugin-mdx": "^1.2.52",
    "gatsby-plugin-offline": "^3.3.2",
    "gatsby-plugin-react-helmet": "^3.3.12",
    "gatsby-plugin-remove-trailing-slashes": "^2.4.0",
    "gatsby-plugin-robots-txt": "^1.5.3",
    "gatsby-plugin-sharp": "^2.6.43",
    "gatsby-plugin-sitemap": "^2.4.17",
    "gatsby-plugin-styled-components": "^3.3.14",
    "gatsby-plugin-typography": "^2.5.12",
    "gatsby-remark-copy-linked-files": "^2.3.19",
    "gatsby-remark-embedder": "^4.1.0",
    "gatsby-remark-images": "^3.5.1",
    "gatsby-remark-prismjs": "^3.5.16",
    "gatsby-source-filesystem": "^2.3.35",
    "gatsby-transformer-sharp": "^2.5.20",
    "lodash": "^4.17.20",
    "polished": "^4.0.3",
    "prismjs": "^1.22.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^3.11.0",
    "react-typography": "^0.16.19",
    "remark-emoji": "^2.1.0",
    "styled-components": "^5.2.0",
    "styled-media-query": "^2.1.2",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@graphql-codegen/cli": "^1.19.1",
    "@graphql-codegen/typescript": "1.17.11",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.2.0",
    "@types/faker": "^5.1.4",
    "@types/jest": "^26.0.15",
    "@types/mdx-js__react": "^1.5.3",
    "@types/react-helmet": "^6.1.0",
    "@types/styled-components": "^5.1.4",
    "@types/typography": "^0.16.3",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "babel-jest": "^26.6.1",
    "commitizen": "^4.2.2",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "faker": "^5.1.0",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.1",
    "lint-staged": "^10.4.2",
    "prettier": "2.1.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.0.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  }
}
