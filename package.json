{
  "name": "talohana.com",
  "private": true,
  "version": "1.0.0",
  "author": "Tal Ohana <tal.ohana.x@gmail.com>",
  "engines": {
    "node": ">= 14.15.0"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx,css,scss,yaml,yml}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "commit": "git-cz",
    "codegen": "graphql-codegen --config codegen.yml",
    "postcodegen": "prettier --write ./src/models/generated.ts"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "gatsby": "^2.32.4",
    "gatsby-plugin-google-analytics": "^2.10.0",
    "gatsby-plugin-image": "^0.7.1",
    "gatsby-plugin-manifest": "^2.11.0",
    "gatsby-plugin-mdx": "^1.9.0",
    "gatsby-plugin-netlify": "^2.11.0",
    "gatsby-plugin-offline": "^3.10.1",
    "gatsby-plugin-react-helmet": "^3.9.0",
    "gatsby-plugin-robots-txt": "^1.5.5",
    "gatsby-plugin-sharp": "^2.14.1",
    "gatsby-plugin-sitemap": "^2.11.0",
    "gatsby-plugin-styled-components": "^3.9.0",
    "gatsby-remark-autolink-headers": "^2.11.0",
    "gatsby-remark-copy-linked-files": "^2.10.0",
    "gatsby-remark-embedder": "^4.1.0",
    "gatsby-remark-images": "^3.11.0",
    "gatsby-remark-prismjs": "^3.13.0",
    "gatsby-source-filesystem": "^2.10.0",
    "gatsby-transformer-sharp": "^2.12.0",
    "lodash": "^4.17.20",
    "postcss": "^8.2.4",
    "prismjs": "^1.23.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-headroom": "^3.1.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.2.0",
    "remark-emoji": "^2.1.0",
    "styled-components": "^5.1.1",
    "tailwindcss": "^2.0.3",
    "twin.macro": "^2.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@graphql-codegen/cli": "^1.20.0",
    "@graphql-codegen/typescript": "^1.20.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.2",
    "@types/faker": "^5.1.5",
    "@types/jest": "^26.0.20",
    "@types/mdx-js__react": "^1.5.3",
    "@types/react-headroom": "^2.2.1",
    "@types/react-helmet": "^6.1.0",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "babel-jest": "^26.6.1",
    "commitizen": "^4.2.3",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "faker": "^5.4.0",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.1",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components"
    }
  }
}
