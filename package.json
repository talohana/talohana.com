{
  "name": "talohana.com",
  "private": true,
  "version": "1.0.0",
  "author": "Tal Ohana <tal.ohana.x@gmail.com>",
  "engines": {
    "node": ">= 14.15.0"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx,css,scss,yaml,yml}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "commit": "git-cz",
    "codegen": "graphql-codegen --config codegen.yml",
    "postcodegen": "prettier --write ./src/types/generated.ts"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.19",
    "@mdx-js/react": "^1.6.19",
    "gatsby": "^2.27.0",
    "gatsby-background-image": "^1.3.1",
    "gatsby-image": "^2.7.0",
    "gatsby-plugin-google-analytics": "^2.6.0",
    "gatsby-plugin-manifest": "^2.7.0",
    "gatsby-plugin-mdx": "^1.5.0",
    "gatsby-plugin-offline": "^3.5.0",
    "gatsby-plugin-react-helmet": "^3.5.0",
    "gatsby-plugin-remove-trailing-slashes": "^2.5.0",
    "gatsby-plugin-robots-txt": "^1.5.3",
    "gatsby-plugin-sharp": "^2.9.0",
    "gatsby-plugin-sitemap": "^2.7.0",
    "gatsby-plugin-styled-components": "^3.3.14",
    "gatsby-remark-copy-linked-files": "^2.5.0",
    "gatsby-remark-embedder": "^4.1.0",
    "gatsby-remark-images": "^3.6.0",
    "gatsby-remark-images-medium-zoom": "^1.7.0",
    "gatsby-remark-prismjs": "^3.9.0",
    "gatsby-source-filesystem": "^2.6.1",
    "gatsby-transformer-sharp": "^2.7.0",
    "lodash": "^4.17.20",
    "postcss": "^8.2.1",
    "prismjs": "^1.22.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.1.0",
    "remark-emoji": "^2.1.0",
    "styled-components": "^5.1.1",
    "tailwindcss": "^2.0.2",
    "twin.macro": "^2.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@graphql-codegen/cli": "^1.19.2",
    "@graphql-codegen/typescript": "1.17.11",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.2.0",
    "@types/faker": "^5.1.4",
    "@types/jest": "^26.0.15",
    "@types/mdx-js__react": "^1.5.3",
    "@types/react-helmet": "^6.1.0",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-jest": "^26.6.1",
    "commitizen": "^4.2.2",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "faker": "^5.1.0",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.1",
    "lint-staged": "^10.5.2",
    "prettier": "2.2.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components"
    }
  }
}
